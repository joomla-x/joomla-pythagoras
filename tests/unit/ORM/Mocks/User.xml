<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE entity SYSTEM
    "https://github.com/nibralab/joomla-architecture/blob/master/code/Joomla/ORM/Definition/entity.dtd">
<entity name="Master">

    <fields>
        <field name="id" type="id"/>
        <field name="username" type="title"/>
    </fields>

    <relations>
        <belongsTo
            name="aggregate_root_id"
            entity="User"
        />
        <belongsTo
            name="second_aggregate_root_id"
            entity="User"
        />
    </relations>

</entity>
